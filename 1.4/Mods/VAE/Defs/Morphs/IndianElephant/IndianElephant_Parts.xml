<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation= "https://raw.githubusercontent.com/Tachyonite/Pawnmorpher/dev/Schemas/MutationDef.xsd">
	<Pawnmorph.Hediffs.MutationDef ParentName="ElephantidaeJawPart">
		<classInfluence>PMAE_IndianElephantMorph</classInfluence>
		<defName>PMAE_IndianElephantJaw</defName>
		<label>indian elephant trunk</label>
		<graphics>
			<MuzzleMale>Parts/TropicalSwamp/IndianElephant/Ielephant_Muzzle_Male</MuzzleMale>
			<MuzzleFemale>Parts/TropicalSwamp/IndianElephant/Ielephant_Muzzle_Female</MuzzleFemale>
		</graphics>
	</Pawnmorph.Hediffs.MutationDef>

	<Pawnmorph.Hediffs.MutationDef ParentName="ElephantidaeEarPart">
		<classInfluence>PMAE_IndianElephantMorph</classInfluence>
		<defName>PMAE_IndianElephantEar</defName>
		<label>indian elephant ear</label>
		<graphics>
			<LeftEar>Parts/TropicalSwamp/IndianElephant/Ielephant_Ear_Left</LeftEar>
			<LeftEarBehind>Parts/None/None</LeftEarBehind>
			<RightEar>Parts/TropicalSwamp/IndianElephant/Ielephant_Ear_Right</RightEar>
			<RightEarBehindHead>Parts/None/None</RightEarBehindHead>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<description>Wow, with such a big ear, [PAWN_nameDef] looks like a dumbo, doesn't [PAWN_pronoun].</description>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>
	
	<Pawnmorph.Hediffs.MutationDef ParentName="ElephantidaeTailPart">
		<classInfluence>PMAE_IndianElephantMorph</classInfluence>
		<defName>PMAE_IndianElephantTail</defName>
		<label>indian elephant tail</label>
		<graphics>
			<Tail>Parts/TropicalSwamp/IndianElephant/Ielephant_Tail</Tail>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<description>A long tail that swishes gently at flies that gather.</description>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>
	
</Defs>
