<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation= "https://raw.githubusercontent.com/Tachyonite/Pawnmorpher/dev/Schemas/MutationDef.xsd">	
	<Pawnmorph.Hediffs.MutationDef ParentName="CrocodiliaJawPart">
		<classInfluence>PMAE_AlligatorMorph</classInfluence>
		<defName>PMAE_AlligatorJaw</defName>
		<label>alligator snout</label>
		<graphics>
			<MuzzleMale>Parts/TropicalSwamp/Alligator/Alligator_Muzzle_Male</MuzzleMale>
			<MuzzleFemale>Parts/TropicalSwamp/Alligator/Alligator_Muzzle_Female</MuzzleFemale>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>adapting</stageKey>
				<values>
					<description>[PAWN_nameDef]'s face has elongated into a scaly snout, like an alligator. The strange shape makes speaking very difficult.</description>
				</values>
			</li>
			<li function="modify">
				<stageKey>adapted</stageKey>
				<values>
					<description>[PAWN_nameDef]'s face has elongated into a scaly snout, like an alligator. Adapting to it has made speaking less frustrating.</description>
				</values>
			</li>
			<li function="modify">
				<stageKey>paragon</stageKey>
				<values>
					<description>[PAWN_nameDef]'s face has elongated into a scaly snout, like an alligator. With further adaptation, [PAWN_nameDef] has learned to speak normally.</description>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>

	<Pawnmorph.Hediffs.MutationDef ParentName="CrocodiliaTailPart">
		<classInfluence>PMAE_AlligatorMorph</classInfluence>
		<defName>PMAE_AlligatorTail</defName>
		<label>alligator tail</label>
		<graphics>
			<Tail>Parts/TropicalSwamp/Alligator/Alligator_Tail</Tail>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<description>TODO Alligator</description>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>
	
</Defs>
