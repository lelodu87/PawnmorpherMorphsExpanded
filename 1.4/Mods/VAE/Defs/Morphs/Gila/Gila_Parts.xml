<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/Tachyonite/Pawnmorpher/dev/Schemas/MutationDef.xsd">
	<Pawnmorph.Hediffs.MutationDef ParentName="LacertiliaJawPart">
		<classInfluence>PME_GilaMorph</classInfluence>
		<defName>PME_GilaMuzzle</defName>
		<label>gila muzzle</label>
		<graphics>
			<MuzzleMale>Parts/Desert/Gila/Gila_Muzzle_Male</MuzzleMale>
			<MuzzleFemale>Parts/Desert/Gila/Gila_Muzzle_Female</MuzzleFemale>
		</graphics>
		<mutationMemory>PME_GilaSnout</mutationMemory>
	</Pawnmorph.Hediffs.MutationDef>

	<Pawnmorph.Hediffs.MutationDef ParentName="LacertiliaTailPart">
		<classInfluence>PME_GilaMorph</classInfluence>
		<defName>PME_GilaTail</defName>
		<label>gila tail</label>
		<graphics>
			<Tail>Parts/Desert/Gila/Gila_Tail</Tail>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<description>A thick, muscular tail has emerged at the end of [PAWN_nameDef]'s spine. It's very tough and can easily take a hit without getting seriously hurt.</description>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>
</Defs>
