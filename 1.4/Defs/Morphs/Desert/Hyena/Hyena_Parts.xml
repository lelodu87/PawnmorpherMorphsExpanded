<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/Tachyonite/Pawnmorpher/dev/Schemas/MutationDef.xsd">
	<Pawnmorph.Hediffs.MutationDef ParentName="ScavengerJawPart">
		<mutationMemory>PME_HyenaMuzzle</mutationMemory>
		<classInfluence>PME_HyenaMorph</classInfluence>
		<defName>PME_HyenaMuzzle</defName>
		<label>hyena muzzle</label>
		<graphics>
			<MuzzleMale>Parts/Desert/Hyena/Hyena_Muzzle_Male</MuzzleMale>
			<MuzzleFemale>Parts/Desert/Hyena/Hyena_Muzzle_Female</MuzzleFemale>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>adapting</stageKey>
				<values>
					<description>A snout graces [PAWN_nameDef]'s face, like that of a hyena. [PAWN_possessive] teeth are pointed to suit. The strange shape makes speaking very difficult.</description>
				</values>
			</li>
			<li function="modify">
				<stageKey>adapted</stageKey>
				<values>
					<description>A snout graces [PAWN_nameDef]'s face, like that of a hyena. [PAWN_possessive] teeth are pointed to suit. Adapting to it has made speaking less frustrating.</description>
				</values>
			</li>
			<li function="modify">
				<stageKey>paragon</stageKey>
				<values>
					<description>A snout graces [PAWN_nameDef]'s face, like that of a hyena. [PAWN_possessive] teeth are pointed to suit. With further adaptation, [PAWN_nameDef] has learned to speak normally.</description>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>

	<Pawnmorph.Hediffs.MutationDef ParentName="ScavengerEarPart">
		<mutationMemory>PME_HyenaEar</mutationMemory>
		<classInfluence>PME_HyenaMorph</classInfluence>
		<defName>PME_HyenaEar</defName>
		<label>hyena ear</label>
		<graphics>
			<LeftEar>Parts/Desert/Hyena/Hyena_Ear_Left</LeftEar>
			<LeftEarBehind>Parts/None/None</LeftEarBehind>
			<RightEar>Parts/Desert/Hyena/Hyena_Ear_Right</RightEar>
			<RightEarBehindHead>Parts/None/None</RightEarBehindHead>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<description>[PAWN_nameDef]'s ear has grown into a furred, rounded, hyena ear.</description>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>

	<Pawnmorph.Hediffs.MutationDef ParentName="ScavengerTailPart">
		<mutationMemory>PME_HyenaTail</mutationMemory>
		<classInfluence>PME_HyenaMorph</classInfluence>
		<defName>PME_HyenaTail</defName>
		<label>hyena tail</label>
		<graphics>
			<Tail>Parts/Desert/Hyena/Hyena_Tail</Tail>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<description>A thin tail with a tuft of fur at the end.</description>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>
	
</Defs>
