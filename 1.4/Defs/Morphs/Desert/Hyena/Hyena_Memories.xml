<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/Tachyonite/Pawnmorpher/dev/Schemas/ThoughtDef.xsd">    
	<ThoughtDef ParentName="OutlookMemoryBase">
		<defName>PME_HyenaTransformationMemory</defName>
        <stages>
            <li>
                <label>morphed</label>
                <description>I... Ehehehe... Can’t stop... Ehehehe... Laughing...</description>
                <baseMoodEffect>-3</baseMoodEffect>
            </li>
            <li>
                <label>morphed</label>
                <description>Ehehehe... Just thought of a joke!</description>
                <baseMoodEffect>+3</baseMoodEffect>
            </li>
            <li>
                <label>morphed</label>
                <description>Ehehehe... THIS IS NOT FUNNY!</description>
                <baseMoodEffect>-5</baseMoodEffect>
            </li>
            <li>
                <label>morphed</label>
                <description>Ehehehe!</description>
                <baseMoodEffect>+1</baseMoodEffect>
            </li>
        </stages>
    </ThoughtDef>
	
    <ThoughtDef ParentName="OutlookMemoryBase">
		<defName>PME_HyenaRevertedMemory</defName>
        <stages>
            <li>
                <label>reverted</label>
                <description>A breather... Laughing all the time was exhausting.</description>
                <baseMoodEffect>3</baseMoodEffect>
            </li>
            <li>
                <label>reverted</label>
                <description>Everything is way less funny now...</description>
                <baseMoodEffect>-3</baseMoodEffect>
            </li>
            <li>
                <label>reverted</label>
                <description>Who’s having the last laugh now?</description>
                <baseMoodEffect>10</baseMoodEffect>
            </li>
            <li>
                <label>reverted</label>
                <description>Fun killers...</description>
				<baseMoodEffect>-5</baseMoodEffect>
            </li>
        </stages>
    </ThoughtDef>
	
	<ThoughtDef ParentName="OutlookMemoryBase">
        <durationDays>1</durationDays>
        <defName>PME_HyenaMuzzle</defName>
        <stages>
            <li>
                <label>Hyena snout</label>
                <description>I accidentally bit off the spoon...</description>
                <visible>true</visible>
                <baseMoodEffect>-1</baseMoodEffect>
            </li>
            <li>
                <label>Hyena snout</label>
                <description>I can chew metal now!</description>
                <visible>true</visible>
                <baseMoodEffect>2</baseMoodEffect>
            </li>
            <li>
                <label>Hyena snout</label>
                <description>I should not bite my tongue now...</description>
                <visible>true</visible>
                <baseMoodEffect>-3</baseMoodEffect>
            </li>
        </stages>
    </ThoughtDef>
	
    <ThoughtDef ParentName="OutlookMemoryBase">
        <durationDays>1</durationDays>
        <defName>PME_HyenaEar</defName>
        <stages>
            <li>
                <label>Hyena ears</label>
                <description>They're a bit too sensitive, everything's too loud for comfort.</description>
                <visible>true</visible>
                <baseMoodEffect>-1</baseMoodEffect>
            </li>
            <li>
                <label>Hyena ears</label>
                <description>My hearing is so clear now, I bet I could hear a pin drop on the other side of the colony!</description>
                <visible>true</visible>
                <baseMoodEffect>1</baseMoodEffect>
            </li>
            <li>
                <label>Hyena ears</label>
                <description>I swear I can hear them laughing at me. Why is this happening to me?</description>
                <visible>true</visible>
                <baseMoodEffect>-2</baseMoodEffect>
            </li>
        </stages>
    </ThoughtDef>
	
    <ThoughtDef ParentName="OutlookMemoryBase">
        <durationDays>1</durationDays>
        <defName>PME_HyenaTail</defName>
        <stages>
            <li>
                <label>Hyena tail</label>
                <description>I hope making a tail-hole in my pants won't be too hard.</description>
                <visible>true</visible>
                <baseMoodEffect>-1</baseMoodEffect>
            </li>
            <li>
                <label>Hyena tail</label>
                <description>Whenever I think about it it just starts to wag!</description>
                <visible>true</visible>
                <baseMoodEffect>1</baseMoodEffect>
            </li>
            <li>
                <label>Hyena tail</label>
                <description>I can't even sit on a chair without this damn thing getting in the way!</description>
                <visible>true</visible>
                <baseMoodEffect>-2</baseMoodEffect>
            </li>
        </stages>
    </ThoughtDef>
</Defs>