<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/Tachyonite/Pawnmorpher/dev/Schemas/MorphDef.xsd">
	<Pawnmorph.MorphDef ParentName="WildBovidMorph">
		<defName>PME_WildebeestMorph</defName>
		<label>wildebeestmorph</label>
		<description>The Wildebeestmorph is the wild and tough cousin of the domestic cowmorph. Its mean temper complements the sharp horns that can be honed by those with an affinity for mutation.</description>
		<race>AEXP_Wildebeest</race>
		<fullTransformation>PME_WildebeestTF</fullTransformation>
		<partialTransformation>PME_WildebeestTFPartial</partialTransformation>
		<raceSettings>
			<graphicsSettings>
				<skinColorOverride>(148, 105, 74)</skinColorOverride>
				<hairColorOverride>(51, 51, 51)</hairColorOverride>
			</graphicsSettings>
		</raceSettings>
		<transformSettings>
			<transformationMemory>PME_WildebeestTransformationMemory</transformationMemory>
			<revertedMemory>PME_WildebeestRevertedMemory</revertedMemory>
		</transformSettings>
	</Pawnmorph.MorphDef>
</Defs>