<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation= "https://raw.githubusercontent.com/Tachyonite/Pawnmorpher/dev/Schemas/MutationDef.xsd">
	<Pawnmorph.Hediffs.MutationDef ParentName="EquineJawPart">
		<classInfluence>PME_ZebraMorph</classInfluence>
		<defName>PME_ZebraJaw</defName>
		<label>zebra snout</label>
		<graphics>
			<MuzzleMale>Parts/AridShrubland/Zebra/Zebra_Muzzle_Male</MuzzleMale>
			<MuzzleFemale>Parts/AridShrubland/Zebra/Zebra_Muzzle_Female</MuzzleFemale>
		</graphics>
	</Pawnmorph.Hediffs.MutationDef>
	
		<Pawnmorph.Hediffs.MutationDef ParentName="EquineEarPart">
		<classInfluence>PME_ZebraMorph</classInfluence>
		<defName>PME_ZebraEar</defName>
		<label>zebra ear</label>
		<graphics>
			<LeftEar>Parts/AridShrubland/Zebra/Zebra_Ear_Left</LeftEar>
			<LeftEarBehind>Parts/None/None</LeftEarBehind>
			<RightEar>Parts/AridShrubland/Zebra/Zebra_Ear_Right</RightEar>
			<RightEarBehindHead>Parts/None/None</RightEarBehindHead>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<description>A pointed, equine ear has grown on top of [PAWN_nameDef]'s head. Because it's able to rotate it hears slightly better than a human, but is mostly decorative.</description>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>

	<Pawnmorph.Hediffs.MutationDef ParentName="EquineTailPart">
		<classInfluence>PME_ZebraMorph</classInfluence>
		<defName>PME_ZebraTail</defName>
		<label>zebra tail</label>
		<graphics>
			<Tail>Parts/AridShrubland/Zebra/Zebra_Tail</Tail>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<description>[PAWN_nameDef]'s spine now ends in a thin tail decorated with long strands. Useful for whipping off flies but not much else.</description>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>
	
</Defs>
