<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/Tachyonite/Pawnmorpher/dev/Schemas/MorphDef.xsd">
	<Pawnmorph.MorphDef ParentName="OphidiaMorph">
		<defName>PME_RattlesnakeMorph</defName>
		<label>rattlesnakemorph</label>
		<description>Unlike popular belief the Rattlesnakemorph only uses its rattle to ward off foes to preserving its incapacitating venom. (Somewhat) silent and deadly.</description>
		<race>AEXP_Rattlesnake</race>
		<categories>
			<li>Arid</li>
		</categories>
		<fullTransformation>PME_RattlesnakeTF</fullTransformation>
		<partialTransformation>PME_RattlesnakeTFPartial</partialTransformation>
		<raceSettings>
			<graphicsSettings>
				<skinColorOverride>(99,85,74)</skinColorOverride>
				<hairColorOverride>(0,0,0,0)</hairColorOverride>
			</graphicsSettings>
			<explicitHybridRace>PMEA_RattlesnakeMorph</explicitHybridRace>
			<transferHumanBodyAddons>true</transferHumanBodyAddons>
			<requiredMutations>
				<li>PME_RattlesnakeTail</li>
			</requiredMutations>
		</raceSettings>
		<transformSettings>
			<transformationMemory>PME_RattlesnakeTransformationMemory</transformationMemory>
			<revertedMemory>PME_RattlesnakeRevertedMemory</revertedMemory>
		</transformSettings>
	</Pawnmorph.MorphDef>
</Defs>
