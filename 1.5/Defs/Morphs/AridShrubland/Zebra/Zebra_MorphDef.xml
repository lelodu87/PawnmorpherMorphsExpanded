<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/Tachyonite/Pawnmorpher/dev/Schemas/MorphDef.xsd">
	<Pawnmorph.MorphDef ParentName="EquineMorph">
		<defName>PME_ZebraMorph</defName>
		<label>zebramorph</label>
		<description>The Zebramorph might look like a Horsemorph but has white stripes instead. No one is sure why, but it looks pretty!</description>
		<race>AEXP_Zebra</race>
		<fullTransformation>PME_ZebraTF</fullTransformation>
		<partialTransformation>PME_ZebraTFPartial</partialTransformation>
		<raceSettings>
			<graphicsSettings>
				<skinColorOverride>(255, 255, 255)</skinColorOverride>
				<hairColorOverride>(38, 38, 38)</hairColorOverride>
			</graphicsSettings>
		</raceSettings>
		<transformSettings>
			<transformationMemory>PME_ZebraTransformationMemory</transformationMemory>
			<revertedMemory>PME_ZebraRevertedMemory</revertedMemory>
		</transformSettings>
	</Pawnmorph.MorphDef>
</Defs>