<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/Tachyonite/Pawnmorpher/dev/Schemas/MutationDef.xsd">
	<Pawnmorph.Hediffs.MutationDef Name ="ApeJawPart" ParentName="SimianJawPart" Abstract="true">
		<comps>
			<li Class="HediffCompProperties_VerbGiver">
				<tools>
					<li>
						<label>teeth</label>
						<labelUsedInLogging>False</labelUsedInLogging>
						<capacities>
							<li>Bite</li>
						</capacities>
						<power>8.0</power>
						<cooldownTime>1.5</cooldownTime>
						<alwaysTreatAsWeapon>true</alwaysTreatAsWeapon>
					</li>
				</tools>
			</li>
		</comps>
		<stagePatches>
			<li function="initial">
				<stageKey>pressing</stageKey>
				<values>
					<verbOverrides>
						<li>
							<label>teeth</label>
							<power>0</power>
						</li>
					</verbOverrides>
				</values>
			</li>
			<li function="modify">
				<stageKey>pressing</stageKey>
				<values>
					<verbOverrides>
						<li>
							<label>teeth</label>
							<power>0</power>
						</li>
					</verbOverrides>
				</values>
			</li>
			<li function="modify">
				<stageKey>growing</stageKey>
				<values>
					<verbOverrides>
						<li>
							<label>teeth</label>
							<power>0</power>
						</li>
					</verbOverrides>
				</values>
			</li>
			<li function="modify">
				<stageKey>adapting</stageKey>
				<values>
					<verbOverrides>
						<li>
							<label>teeth</label>
							<power>4</power>
						</li>
					</verbOverrides>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>

	<Pawnmorph.Hediffs.MutationDef Name ="ApeEarPart" ParentName="SimianEarPart" Abstract="true">
	</Pawnmorph.Hediffs.MutationDef>

	<!-- No tail -->
	
</Defs>